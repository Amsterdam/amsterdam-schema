      "storingType": {
        "type": "string",
        "description": "Type stroomstoring."
      },
      "storingEnergiesoort": {
        "type": "string",
        "description": "Energiesoort met betrekking tot stroomstoring."
      },
      "storingStatus": {
        "type": "string",
