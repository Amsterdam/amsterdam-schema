      "mutatieActie": {
        "type": "string",
        "description": "Soort handeling uitgevoerd."
      },
      "datumEind": {
        "type": "string",
        "format": "date-time",
        "provenance": "date_end",
        "description": "Datum en tijdstip opgelost."
