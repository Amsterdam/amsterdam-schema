{
  "type": "dataset",
  "id": "deelmobiliteit",
  "title": "Deelmobiliteit",
  "description": "Locaties en contextuele informatie over deelvoertuigen zoals autoâ€™s, fietsen en scooters.",
  "license": "public",
  "status": "beschikbaar",
  "version": "0.0.1",
  "theme": ["deelmobiliteit", "Ruimte en Topografie"],
  "publisher": "OIS",
