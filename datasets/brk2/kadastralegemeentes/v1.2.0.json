{
  "id": "kadastralegemeentes",
  "type": "table",
  "version": "1.2.0",
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "mainGeometry": "geometrie",
    "identifier": "identificatie",
    "required": [
      "schema",
      "identificatie"
    ],
    "display": "identificatie",
    "properties": {
      "schema": {
        "$ref": "https://schemas.data.amsterdam.nl/schema@v1.3.0#/definitions/schema"
      },
      "identificatie": {
        "type": "string",
        "description": "De unieke aanduiding van een Kadastrale gemeente."
      },
      "ligtInBrkGemeente": {
        "type": "object",
        "properties": {
          "identificatie": {
            "type": "string"
          },
          "volgnummer": {
            "type": "integer"
          },
          "beginGeldigheid": {
            "type": "string",
            "format": "date-time"
          },
          "eindGeldigheid": {
            "type": "string",
            "format": "date-time"
          }
        },
        "relation": "brk2:gemeentes",
        "description": "De burgerlijke gemeente waarin de kadastrale gemeente ligt."
      },
      "ligtInBrkGemeenteCode": {
        "type": "string",
        "description": "De kadastrale gemeente code."
      },
      "geometrie": {
        "$ref": "https://geojson.org/schema/Geometry.json",
        "description": "Vorm en ligging van de kadastrale gemeente in het stelsel van de Rijksdriehoekmeting (RD)."
      }
    }
  }
}
