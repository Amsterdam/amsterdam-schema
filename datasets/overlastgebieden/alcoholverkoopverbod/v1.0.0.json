{
    "id": "alcoholverkoopverbod",
    "title": "Alcoholverkoopverbodgebied",
    "type": "table",
    "version": "1.0.0",
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "additionalProperties": false,
        "required": [
            "schema",
            "id"
        ],
        "display": "naam",
        "properties": {
            "schema": {
                "$ref": "https://schemas.data.amsterdam.nl/schema@v2#/definitions/schema"
            },
            "id": {
                "type": "integer",
                "description": "Unieke aanduiding van het overlastgebied."
            },
            "geometry": {
                "$ref": "https://geojson.org/schema/Geometry.json",
                "description": "Multi-Polygoon definitie van het overlastgebied."
            },
            "type": {
                "type": "string",
                "description": "Code die het type overlastgebied aangeeft: (AVG) raamsluitingsverbodgebied."
            },
            "naam": {
                "type": "string",
                "provenance": "oov_naam",
                "description": "Naam zoals gedefinieerd door Openbare Orde en Veiligheid (OOV) van het overlastgebied van toepassing."
            },
            "geldigheid": {
                "type": "string",
                "description": "Beschrijving van de geldigheidsduurperiode."
            },
            "tijd": {
                "type": "string",
                "description": "Beschrijving van de geldigheidsduur in tijd."
            },
            "url": {
                "type": "string",
                "format": "uri",
                "description": "URL naar webpagina voor meer informatie."
            }
        }
    }
}