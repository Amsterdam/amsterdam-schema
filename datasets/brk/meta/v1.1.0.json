{
  "id": "meta",
  "type": "table",
  "version": "1.1.0",
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "identifier": "metaId",
    "required": [
      "schema",
      "metaId"
    ],
    "display": "metaId",
    "properties": {
      "schema": {
        "$ref": "https://schemas.data.amsterdam.nl/schema@v1.2.0#/definitions/schema"
      },
      "metaId": {
        "type": "string",
        "description": "BRK Meta ID",
        "provenance": "id"
      },
      "kennisgevingsdatum": {
        "type": "string",
        "format": "date-time",
        "description": "Toestandsdatum, d.i. de laatste aanmaakdatum van de BRK-berichten in de naar DIVA gerepliceerde BRK-levering."
      }
    }
  }
}
