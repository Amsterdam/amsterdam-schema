{
    "id": "brondataset",
    "type": "table",
    "version": "1.0.0",
    "title": "brondataset",
    "description": "Brondataset.",
    "identifier": "id",
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "additionalProperties": false,
        "required": [
            "schema",
            "id"
        ],
        "display": "id",
        "properties": {
            "schema": {
                "$ref": "https://schemas.data.amsterdam.nl/schema@v2#/definitions/schema"
            },
            "id": {
                "type": "string",
				"title": "Id",
                "description": "Uniek identificerend kenmerk voor de brondataset."
            },
            "naam": {
                "type": "string",
                "title": "Naam",
                "description": "Naam van de betreffende brondataset."
            },
            "omschrijving": {
                "type": "string",
                "title": "Omschrijving brondataset",
                "description": "Omschrijving van / toelichting op de betreffende brondataset."
            },
            "bronhouder": {
                "type": "string",
		"title": "bronhouder",
                "description": "organisatieonderdeel dat bronhouder is van de betreffende brondataset."
            },
            "status": {
                "type": "integer",
                "title": "Status",
                "description": "Status van de brondataset (-1 = afgevoerd, 0= inactief, 1 = actief)."
            },
            "datum": {
                "type": "string",
				"format": "date",
                "title": "Datum",
                "description": "Datum van de statuswijziging."
            }			
        }
    }
}
