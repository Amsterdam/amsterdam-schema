{
    "id": "olKgKengetallen",
    "type": "table",
    "title": "olKgKengetallen",
    "description": "Leerlingen, prognoses en gebruik in vierkante meters per schoolgebouw",
    "version": "1.0.0",
    "lifecycleStatus": "stable",
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "required": [
            "schema"
        ],
        "display": "kgId",
        "properties": {
            "schema": {
                "$ref": "https://schemas.data.amsterdam.nl/schema@v3.1.0#/definitions/schema"
            },
            "id": {
                "type": "string",
                "description": "Unieke identificatiecode van een kengetal, gebruikt binnen Databricks om een specifieke kengetal eenduidig te identificeren."
            },
            "kgId": {
                "type": "string",
                "description": "Unieke identificatiecode van de kengetal vanuit de bronapplicatie"
            },
            "inId": {
                "type": "string",
                "description": "Verwijzing naar de gekoppelde instelling."
            },
            "kgPrognoseSoort": {
                "type": "string",
                "description": "Soort prognose gekoppeld aan de kengetallen."
            },
            "kgPrognoseJaar": {
                "type": "string",
                "description": "Prognosejaar gekoppeld aan de kengetallen."
            },
            "kgJaar": {
                "type": "string",
                "description": "Jaar waarop de kengetallen betrekking hebben."
            },
            "kgMaand": {
                "type": "string",
                "description": "Maand waarop de kengetallen betrekking hebben."
            },
            "kgPrognose": {
                "type": "string",
                "description": "Prognosewaarde gekoppeld aan de kengetallen."
            },
            "kgGebruikM2": {
                "type": "string",
                "description": "Gebruiksoppervlakte in vierkante meters (m\u00b2)."
            },
            "kgBehoefteM2": {
                "type": "string",
                "description": "Behoefte aan oppervlakte in vierkante meters (m\u00b2)."
            },
            "kgAantalLeerlingen": {
                "type": "string",
                "description": "Aantal leerlingen gekoppeld aan de kengetallen."
            },
            "kgGebruikKlokuren": {
                "type": "string",
                "description": "Gebruiksuren in klokuren."
            },
            "kgBehoefteKlokuren": {
                "type": "string",
                "description": "Behoefte aan klokuren."
            },
            "kgAchterstandscore": {
                "type": "string",
                "description": "Achterstandscore gekoppeld aan de kengetallen."
            },
            "datumExport": {
                "type": "string",
                "description": "Datum waarop de gegevens van de kengetallen zijn ge\u00ebxporteerd."
            },
            "versie": {
                "type": "string",
                "description": "Versienummer van de kengetallen."
            },
            "mGeldigVan": {
                "type": "string",
                "description": "Startdatum van de geldigheid van de kengetallen.",
                "format": "date-time"
            },
            "mGeldigTot": {
                "type": "string",
                "description": "Einddatum van de geldigheid van de kengetallen.",
                "format": "date-time"
            },
            "mIsActief": {
                "type": "boolean",
                "description": "Geeft aan of de kengetallen actief zijn (Ja/Nee)."
            }
        }
    }
}