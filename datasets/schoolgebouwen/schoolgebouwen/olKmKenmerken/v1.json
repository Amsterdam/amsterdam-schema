{
    "id": "olKmKenmerken",
    "type": "table",
    "title": "olKmKenmerken",
    "description": "Kenmerken van een accomodatie",
    "version": "1.0.0",
    "lifecycleStatus": "stable",
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "required": [
            "schema"
        ],
        "display": "kmOmschrijving",
        "properties": {
            "schema": {
                "$ref": "https://schemas.data.amsterdam.nl/schema@v3.1.0#/definitions/schema"
            },
            "id": {
                "type": "string",
                "description": "Unieke identificatiecode van een kenmerk, gebruikt binnen Databricks om een specifieke kenmerk eenduidig te identificeren."
            },
            "kmId": {
                "type": "string",
                "description": "Unieke identificatiecode van de kenmerk vanuit de bronapplicatie"
            },
            "inId": {
                "type": "string",
                "description": "Verwijzing naar de gekoppelde instelling."
            },
            "bsId": {
                "type": "string",
                "description": "Verwijzing naar de gekoppelde bestuursgegevens."
            },
            "acId": {
                "type": "string",
                "description": "Verwijzing naar de gekoppelde accommodatie."
            },
            "kmOmschrijving": {
                "type": "string",
                "description": "Beschrijving of toelichting van de kenmerken."
            },
            "datumExport": {
                "type": "string",
                "description": "Datum waarop de gegevens van de kenmerken zijn ge\u00ebxporteerd."
            },
            "versie": {
                "type": "string",
                "description": "Versienummer van de kenmerken."
            },
            "mGeldigVan": {
                "type": "string",
                "description": "Startdatum van de geldigheid van de kenmerken.",
                "format": "date-time"
            },
            "mGeldigTot": {
                "type": "string",
                "description": "Einddatum van de geldigheid van de kenmerken.",
                "format": "date-time"
            },
            "mIsActief": {
                "type": "boolean",
                "description": "Geeft aan of de kenmerken actief zijn (Ja/Nee)."
            }
        }
    }
}