{
    "id": "olObObject",
    "type": "table",
    "title": "olObObject",
    "description": "Fysieke ruimtes binnen een accomodatie",
    "version": "1.0.0",
    "lifecycleStatus": "stable",
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "required": [
            "schema"
        ],
        "display": "obType",
        "properties": {
            "schema": {
                "$ref": "https://schemas.data.amsterdam.nl/schema@v3.1.0#/definitions/schema"
            },
            "id": {
                "type": "string",
                "description": "Unieke identificatiecode van een object, gebruikt binnen Databricks om een specifieke object eenduidig te identificeren."
            },
            "obId": {
                "type": "string",
                "description": "Unieke identificatiecode van het object van de bronapplicatie"
            },
            "acId": {
                "type": "string",
                "description": "Verwijzing naar de gekoppelde accommodatie."
            },
            "obCode": {
                "type": "string",
                "description": "Code die het object identificeert."
            },
            "obType": {
                "type": "string",
                "description": "Type object, bijvoorbeeld een specifieke classificatie."
            },
            "obRuimtesoort": {
                "type": "string",
                "description": "Soort ruimtes binnen het object."
            },
            "obRuimtetype": {
                "type": "string",
                "description": "Type ruimte binnen het object."
            },
            "obCapaciteitM2": {
                "type": "string",
                "description": "Capaciteit van het object in vierkante meters (m\u00b2)."
            },
            "obCapaciteitUren": {
                "type": "string",
                "description": "Capaciteit van het object in uren."
            },
            "obCapaciteitRuimtes": {
                "type": "string",
                "description": "Capaciteit van het object in aantal ruimtes."
            },
            "obLeegstandM2": {
                "type": "string",
                "description": "Leegstand van het object in vierkante meters (m\u00b2)."
            },
            "obLeegstandUren": {
                "type": "string",
                "description": "Aantal uren dat ruimtes binnen het object leegstaan."
            },
            "obLeegstandRuimtes": {
                "type": "string",
                "description": "Aantal ruimtes die leegstaan binnen het object."
            },
            "datumExport": {
                "type": "string",
                "description": "Datum waarop de gegevens van het object zijn ge\u00ebxporteerd."
            },
            "versie": {
                "type": "string",
                "description": "Versienummer van het object."
            },
            "mGeldigVan": {
                "type": "string",
                "description": "Startdatum van de geldigheid van het object.",
                "format": "date-time"
            },
            "mGeldigTot": {
                "type": "string",
                "description": "Einddatum van de geldigheid van het object.",
                "format": "date-time"
            },
            "mIsActief": {
                "type": "boolean",
                "description": "Geeft aan of het object actief is (Ja/Nee)."
            }
        }
    }
}