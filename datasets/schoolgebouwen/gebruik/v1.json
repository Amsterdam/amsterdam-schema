{
    "id": "gebruik",
    "type": "table",
    "title": "gebruik",
    "description": "Het gebruik van het schoolgebouw",
    "version": "1.0.2",
    "lifecycleStatus": "stable",
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "identifier":"gebruikId",
        "required": [
            "schema"
        ],
        "display": "gebruikId",
        "properties": {
            "schema": {
                "$ref": "https://schemas.data.amsterdam.nl/schema@v3.1.0#/definitions/schema"
            },
            "id": {
                "type": "string",
                "title": "Gebruik databricks ID",
                "description": "Unieke identificatiecode van een accommodatie, gebruikt binnen Databricks om een specifieke accommodatie eenduidig te identificeren."
            },
            "gebruikId": {
                "type": "string",
                "title": "Gebruik ID",
                "description": "Unieke identificatiecode voor een gebruik vanuit de bronapplicatie"
            },
            "objectId": {
                "type": "string",
                "title": "Gebruik object ID",
                "description": "Verwijzing naar het gekoppelde object.",
                "relation":"schoolgebouwen:object"
            },
            "instellingId": {
                "type": "string",
                "title": "Gebruik instelling ID",
                "description": "Verwijzing naar de gekoppelde instelling.",
                "relation":"schoolgebouwen:instelling"
            },
            "datumBegin": {
                "type": "string",
                "title": "Gebruik datum begin",
                "description": "Startdatum van het gebruik."
            },
            "datumEind": {
                "type": "string",
                "title": "Gebruik datum eind",
                "description": "Einddatum van het gebruik."
            },
            "gebruikM2": {
                "type": "string",
                "title": "Gebruik m2",
                "description": "Gebruiksoppervlakte in vierkante meters (m\u00b2)."
            },
            "gebruikUren": {
                "type": "string",
                "title": "Gebruik uren",
                "description": "Aantal uren van het gebruik."
            },
            "gebruikRuimtes": {
                "type": "string",
                "title": "Gebruik ruimtes",
                "description": "Aantal ruimtes dat in gebruik is."
            },
            "gebruik": {
                "type": "string",
                "title": "Gebruik beschrijving",
                "description": "Beschrijving van het gebruik."
            },
            "datumExport": {
                "type": "string",
                "title": "Gebruik exportdatum",
                "description": "Datum waarop de gegevens van het gebruik zijn ge\u00ebxporteerd."
            },
            "versie": {
                "type": "string",
                "title": "Gebruik versie",
                "description": "Versienummer van het gebruik."
            },
            "mGeldigVan": {
                "type": "string",
                "title": "Gebruik geldig vanaf",
                "description": "Startdatum van de geldigheid van het gebruik.",
                "format": "date-time"
            },
            "mGeldigTot": {
                "type": "string",
                "title": "Gebruik geldig tot",
                "description": "Einddatum van de geldigheid van het gebruik.",
                "format": "date-time"
            },
            "mIsActief": {
                "type": "boolean",
                "title": "Gebruik actieve status",
                "description": "Geeft aan of het gebruik actief is (Ja/Nee)."
            }
        }
    }
}