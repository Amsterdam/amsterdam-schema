{
  "type": "dataset",
  "id": "blackspots",
  "title": "blackspots",
  "status": "beschikbaar",
  "description": "Blackspots",
  "auth": "FP/MDW",
  "version": "0.0.2",
  "crs": "EPSG:4326",
  "tables": [
    {
      "id": "blackspots",
      "type": "table",
      "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "type": "object",
        "additionalProperties": false,
        "required": [
          "schema",
          "id",
          "locatie id",
          "spot type",
          "geometry",
          "stadsdeel",
          "status"
        ],
        "display": "id",
        "properties": {
          "schema": {
            "$ref": "https://schemas.data.amsterdam.nl/schema@v1.1.1#/definitions/schema"
          },
          "id": {
            "type": "integer"
          },
          "locatie id": {
            "type": "string"
          },
          "spot type": {
            "type": "string",

            "enum": ["protocol ernstig", "blackspot", "risico", "protocol dodelijk", "wegvak"]
          },
          "geometry": {
            "$ref": "https://geojson.org/schema/Point.json"
          },
          "stadsdeel": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "enum": [
              "voorbereiding",
              "onderzoek ontwerp",
              "geen maatregel",
              "gereed",
              "uitvoering"
            ]
          },
          "start uitvoering": {
            "type": "string",
            "format": "date"
          },
          "eind uitvoering": {
            "type": "string",
            "format": "date"
          },
          "start opmerking": {
            "type": "string"
          },
          "eind opmerking": {
            "type": "string"
          },
          "jaar blackspotlijst": {
            "type": "integer",
            "unit": "a_g"
          },
          "jaar ongeval quickscan": {
            "type": "integer",
            "unit": "a_g"
          },
          "jaar oplevering": {
            "type": "integer",
            "unit": "a_g"
          }
        }
      }
    }
  ]
}
