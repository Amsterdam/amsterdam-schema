  "description": "URI en metadata processenverbaal in pdf per jaar",
  "type": "table",
  "version": "1.0.0",
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
      "schema",
      "id"
    ],
    "display": "id",
    "properties": {
      "schema": {
        "$ref": "https://schemas.data.amsterdam.nl/schema@v3.1.0#/definitions/schema"
      },
      "id": {
