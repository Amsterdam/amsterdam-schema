{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://schemas.data.amsterdam.nl/v0.1",
  "type": "object",
  "definitions": {
    "id": {
      "type": "string",
      "minLength": 1,
      "pattern": "^[0-9a-z]+([_-]?[0-9a-z]+)*$"
    },
    "dataset": {
      "type": "string"
    },
    "uri": {
      "type": "string",
      "format": "uri"
    },
    "title": {
      "type": "string"
    },
    "year": {
      "type": "integer"
    }
  }
}